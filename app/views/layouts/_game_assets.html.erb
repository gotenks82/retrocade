<% if @play %>
<% if @game.game_version.gameplay_js %><script src="/<%= @game.game_version.gameplay_js.path %>" ></script><% end %>
<% if @game.game_version.custom_css %><link href="/<%= @game.game_version.custom_css.path %>" media="all" rel="stylesheet"><% end %>
<% if @game.game_version.js_libraries %>
    <% @game.game_version.js_libraries.each do |lib| %>
        <script src="<%= lib.path %>" ></script>
    <% end %>
<% end %>


<% end %>